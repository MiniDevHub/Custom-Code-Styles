      "\t<title>${2:Document}</title>",
      "\t<link rel=\"stylesheet\" href=\"${3:style.css}\">",
      "</head>",
      "<body>",
      "\t$0",
      "\t<script src=\"${4:script.js}\" defer></script>",
      "</body>",
      "</html>"
    ],
    "description": "Basic HTML5 boilerplate with CSS and JS"
  },
  "HTML5 Complete Boilerplate": {
    "prefix": "!!!",
    "body": [
      "<!DOCTYPE html>",
      "<html lang=\"${1:en}\">",
      "<head>",
      "\t<meta charset=\"UTF-8\">",
      "\t<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
      "\t<meta http-equiv=\"X-UA-Compatible\" content=\"ie=edge\">",
      "\t",
      "\t<!-- SEO Meta Tags -->",
      "\t<meta name=\"description\" content=\"${2:Page description}\">",
      "\t<meta name=\"keywords\" content=\"${3:keyword1, keyword2, keyword3}\">",
      "\t<meta name=\"author\" content=\"${4:Author Name}\">",
      "\t",
      "\t<!-- Open Graph / Facebook -->",
      "\t<meta property=\"og:type\" content=\"website\">",
      "\t<meta property=\"og:url\" content=\"${5:https://example.com/}\">",
      "\t<meta property=\"og:title\" content=\"${6:Page Title}\">",
      "\t<meta property=\"og:description\" content=\"${2:Page description}\">",
      "\t<meta property=\"og:image\" content=\"${7:https://example.com/image.jpg}\">",
      "\t",
      "\t<!-- Twitter -->",
      "\t<meta property=\"twitter:card\" content=\"summary_large_image\">",
      "\t<meta property=\"twitter:url\" content=\"${5:https://example.com/}\">",
      "\t<meta property=\"twitter:title\" content=\"${6:Page Title}\">",
      "\t<meta property=\"twitter:description\" content=\"${2:Page description}\">",
      "\t<meta property=\"twitter:image\" content=\"${7:https://example.com/image.jpg}\">",
      "\t",
      "\t<title>${6:Page Title}</title>",
      "\t",
      "\t<!-- Favicon -->",
      "\t<link rel=\"icon\" type=\"image/x-icon\" href=\"/favicon.ico\">",
      "\t<link rel=\"apple-touch-icon\" sizes=\"180x180\" href=\"/apple-touch-icon.png\">",
      "\t",
      "\t<!-- Stylesheets -->",
      "\t<link rel=\"stylesheet\" href=\"${8:style.css}\">",
      "</head>",
      "<body>",
      "\t$0",
      "\t",
      "\t<!-- Scripts -->",
      "\t<script src=\"${9:script.js}\" defer></script>",
      "</body>",
      "</html>"
    ],
    "description": "Complete HTML5 boilerplate with SEO and social media tags"
  },
