      "\t\t<label for=\"${4:inputId}\">${5:Label}:</label>",
      "\t\t<input type=\"${6:text}\" id=\"${4:inputId}\" name=\"${7:inputName}\" ${8:required}>",
      "\t</div>",
      "\t$0",
      "\t<button type=\"submit\">${9:Submit}</button>",
      "</form>"
