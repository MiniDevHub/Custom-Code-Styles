    {
      "tag": "!",
      "color": "#FF00FF",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": true,
      "italic": false
    },
    {
      "tag": "TODO:",
      "color": "#EA580C",
      "strikethrough": false,
