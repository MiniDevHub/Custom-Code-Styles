  // ┌─────────────────────────────────────────────────────────────────┐
  // │             🪟 WINDOW & INTERFACE MAGNIFICENCE                  │
  // │         Making your IDE look like it came from 2050             │
  // └─────────────────────────────────────────────────────────────────┘

  // INFO: 🎭 Window Appearance - Because first impressions matter!
  "window.customTitleBarVisibility": "windowed",
  "window.title": "MrDib ${activeEditorMedium} ${rootPath}",
  "window.newWindowDimensions": "inherit",
  "window.nativeFullScreen": true,
  "window.zoomLevel": 1.5,

  // INFO: 💎 Transparency & Visual Effects - That sexy glass effect!
  // WARNING: May cause productivity boost due to sheer awesomeness 🥃
  "apc.electron": {
    "opacity": 0.95,
    "transparent": true,
    "backgroundColor": "#00000000",
    "vibrancy": "ultra-dark"
  },

  // ┌─────────────────────────────────────────────────────────────────┐
  // │                 📁 FILE MANAGEMENT EXCELLENCE                   │
  // │            Because your files deserve VIP treatment             │
  // └─────────────────────────────────────────────────────────────────┘

  // INFO: 💾 Auto-Save Magic - Ctrl+S is so 2010! Your fingers will thank you
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 2000,

  // INFO: 🧹 File Cleanup Squad - Keeping your code pristine & professional
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,

  // INFO: 🗂️ File Associations - Teaching VS Code what's what
  "files.associations": {
    "*.html": "html",
    ".php_cs": "php",
    ".php_cs.dist": "php"
  },

  // INFO: 🗺️ Explorer Settings - Navigate like a pro!
  "files.enableTrash": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.autoReveal": true,

  // INFO: 🙈 File Exclusions - Hide the mundane, show the extraordinary
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },

  // INFO: 👁️ Watcher Exclusions - Don't watch what doesn't matter
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true
  },

  // ┌─────────────────────────────────────────────────────────────────┐
  // │                 ✏️ EDITOR CONFIGURATION PARADISE                │
  // │           Where coding dreams become reality™                   │
  // └─────────────────────────────────────────────────────────────────┘

  // INFO: 🎯 Minimalist Display - Less clutter, more focus, maximum zen
  "breadcrumbs.enabled": false,
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "none",
  "editor.overviewRulerBorder": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.renderControlCharacters": false,
  "editor.colorDecorators": false,
  "editor.lightbulb.enabled": "off",
  "editor.stickyScroll.enabled": false,
  "editor.matchBrackets": "never",

  // INFO: ✨ LEGENDARY CURSOR SETTINGS - The cursor that conquered a thousand bugs!
  //! Witness the glowing beacon of productivity! 🌟
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "apc.stylesheet": {
    ".monaco-editor .cursor": "background: #ffffff !important; box-shadow: 0 0 70px 5px #ffffff, #ffffff 0px 0px 34px 1px; color: #161616 !important"
  },

  // INFO: 🏄 Smooth Scrolling - Butter smooth, like your code should be
  "editor.smoothScrolling": true,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.mouseWheelZoom": true,

  // INFO: 📐 Indentation Perfection - Because spaces matter (literally!)
  "editor.tabSize": 10,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.guides.indentation": true,
  "editor.guides.highlightActiveIndentation": false,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,

  // INFO: 🔦 Selection & Highlighting - Illuminate your code journey
  "editor.selectionHighlight": true,
  "editor.occurrencesHighlight": "singleFile",
  "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",

  // INFO: 🔤 Typography Excellence - Ligatures for the sophisticated developer
  "editor.fontLigatures": true,

  // INFO: 🧠 IntelliSense & Suggestions - Your AI coding companion
  "editor.suggestLineHeight": 30,
  "editor.suggestFontSize": 20,
  "editor.snippetSuggestions": "top",
  "editor.copyWithSyntaxHighlighting": false,

  // INFO: 📝 Snippet settings - Because even snippets deserve formatting
  "[snippets]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },

  // INFO: 🎈 Hover Settings - Patience is a virtue
  "editor.hover.delay": 1500,
  "editor.hover.enabled": true,

  // INFO: 🧭 Navigation Settings - Jump to definition like a ninja!
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",

  // INFO: ♿ Accessibility - We code for everyone
  "editor.accessibilitySupport": "off",

  // ┌─────────────────────────────────────────────────────────────────┐
  // │                  🎨 WORKBENCH & THEME ARTISTRY                  │
  // │              Where aesthetics meet functionality                │
  // └─────────────────────────────────────────────────────────────────┘

  // INFO: 🍬 Theme & Icons - Eye candy that doesn't cause cavities!
  "workbench.colorTheme": "Transparent Color Theme",
  "workbench.iconTheme": "material-icon-theme",

  // INFO: 🎨 Custom Color Palette - Painted with love and precision
  "workbench.colorCustomizations": {
    "editorIndentGuide.background1": "#1E1E1E",
    "terminal.background": "#00000000"
  },

  // INFO: 🏗️ Layout Architecture - Feng Shui for your workspace
  "workbench.sideBar.location": "right",
  "workbench.tree.indent": 10,
  "workbench.statusBar.visible": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": true,
  "workbench.list.smoothScrolling": true,

  // INFO: 📑 Tab Management - Organizing tabs like a boss since 2024
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.labelFormat": "default",
  "workbench.editor.openPositioning": "last",
  "workbench.editor.closeEmptyGroups": true,
  "workbench.editor.showIcons": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.wrapTabs": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.tabActionCloseVisibility": false,

  // ┌─────────────────────────────────────────────────────────────────┐
  // │               🚀 ANIMATIONS & SPECIAL EFFECTS                   │
  // │        Because coding should feel like a video game!            │
  // └─────────────────────────────────────────────────────────────────┘

  // INFO: 🎮 VS Code Animations - Level up your coding experience!
  // WARNING: Side effects may include increased dopamine levels
  "animations.Enabled": true,
  "animations.CursorAnimation": true,
  "animations.Command-Palette": "Fade",
  "animations.Active": "Indent",
  "animations.Install-Method": "Apc Customize UI++",
  "animations.CursorAnimationOptions": {
    "TrailLength": 20
  },

  // INFO: 🎬 Animation Imports - The magic sauce
  "apc.imports": [
    "file://${userHome}/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.7/dist/updateHandler.js"
  ],

  // INFO: 🎪 Legacy Custom CSS - For the rebels who want MORE customization!
  "vscode_custom_css.imports": [
    "file:///Users/dibakar/Documents/Code/CustomCode/CustomCSS.css",
    "file:///Users/dibakar/Documents/Code/CustomCode/CustomJavascript.js",
    "file:///Users/dibakar/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.7/dist/updateHandler.js"
  ],

  // ┌─────────────────────────────────────────────────────────────────┐
  // │                💻 TERMINAL CONFIGURATION HQ                     │
  // │             Your command line, but make it fashion              │
  // └─────────────────────────────────────────────────────────────────┘

  // INFO: 🖥️ Terminal Aesthetics - Making terminal sessions sexy since forever
  "terminal.integrated.fontFamily": "Cascadia Code, MesloLGS NF",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.tabs.title": "${process} ~ MrDib",

  // INFO: ⚡ Terminal Behavior - Smooth as silk, fast as lightning
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.smoothScrolling": true,
  "debug.terminal.clearBeforeReusing": true,
  "python.terminal.focusAfterLaunch": true,

  // ┌─────────────────────────────────────────────────────────────────┐
  // │              🔧 LANGUAGE FORMATTERS & COMPILERS                 │
  // │         Supporting more languages than the United Nations       │
  // └─────────────────────────────────────────────────────────────────┘

  // INFO: 🎯 Default Formatter - Prettier, because ugly code is a crime
  "editor.defaultFormatter": "esbenp.prettier-vscode",

  // INFO: 🌐 HTML Configuration - Making the web beautiful
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },

  // INFO: 🐍 Python Configuration - Taming the snake with style!
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.tabSize": 10
  },
  "python.createEnvironment.trigger": "off",

  // INFO: ☕ Java Configuration - Brewing perfect code since 1995
  "[java]": {
    "editor.defaultFormatter": "redhat.java",
    "editor.formatOnSave": true
  },
  "java.import.generatesMetadataFilesAtProjectRoot": true,

  // INFO: 🔩 C Language Configuration - Old school cool
  "[c]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },

  // INFO: 🐚 Shell Script Configuration - Unix philosophy approved!
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },

  "[dockerfile]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },

  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },

  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },

  // INFO: 🌊 Shell Format Languages - Format ALL the things!
  "shellformat.effectLanguages": [
    "shellscript",
    "dockerfile",
    "dotenv",
    "hosts",
    "jvmoptions",
    "ignore",
    "gitignore",
    "properties",
    "spring-boot-properties",
    "azcli",
    "bats"
  ],

  // INFO: 📓 Notebook Configuration - Jupyter never looked so good
  "notebook.defaultFormatter": "xaver.clang-format",
  "notebook.formatOnSave.enabled": true,
  "notebook.formatOnCellExecution": true,

  // INFO: 📦 JavaScript Import Management - ES6 modules FTW!
  "javascript.updateImportsOnFileMove.enabled": "always",

  // INFO: 🏃 Emmet Language Support - HTML zen coding everywhere!
  "emmet.includeLanguages": {
    "html": "html",
    "django-html": "html",
    "blade": "html",
    "vue-html": "html",
    "vue": "html",
    "react": "html",
    "javascript": "html"
  },

  // ┌─────────────────────────────────────────────────────────────────┐
  // │              ▶️ CODE RUNNER - THE POLYGLOT'S PARADISE           │
  // │         Execute code in 50+ languages with one keystroke!       │
  // └─────────────────────────────────────────────────────────────────┘

  // INFO: 🏃‍♂️ Code Runner General Settings - Run fast, debug faster!
  "code-runner.clearPreviousOutput": true,
  "code-runner.showExecutionMessage": false,
  "code-runner.runInTerminal": true,

  // INFO: 🌍 The Grand Executor Map - Supporting every language under the sun!
  "code-runner.executorMap": {
    //! 🏗️ Compiled Languages - Build it strong!
    "c": "clear && cd $dir && gcc $fileName -o $fileNameWithoutExt && ./$(basename $fileNameWithoutExt)",
    "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "go": "go run",
    "zig": "zig run",

    //! 📜 Scripting Languages - Script your way to success!
    "python": "clear && python3 -u",
    "javascript": "node",
    "typescript": "ts-node",
    "ruby": "ruby",
    "perl": "perl",
    "perl6": "perl6",
    "php": "php",
    "lua": "lua",

    //! 💻 Shell & System Scripts - Command the command line!
    "shellscript": "bash",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "bat": "cmd /c",
    "applescript": "osascript",

    //! 🎓 Functional Languages - For the intellectuals
    "haskell": "runghc",
    "fsharp": "fsi",
    "scala": "scala",
    "clojure": "lein exec",
    "erlang": "escript",
    "lisp": "sbcl --script",
    "scheme": "csi -script",
    "sml": "cd $dir && sml $fileName",

    //! 🆕 Modern Languages - The new kids on the block
    "swift": "swift",
    "kotlin": "kotlin",
    "dart": "dart",
    "julia": "julia",
    "nim": "nim compile --verbosity:0 --hints:off --run",
    "crystal": "crystal",
    "mojo": "mojo run",
    "gleam": "gleam run -m $fileNameWithoutExt",

    //! 🌐 Web Languages - Making the internet pretty
    "coffeescript": "coffee",
    "sass": "sass --style expanded",
    "scss": "scss --style expanded",
    "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",

    //! 🔬 Scientific Languages - For when you need to science!
    "r": "Rscript",
    "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",

    //! 🎪 Exotic Languages - Because why not?
    "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    "groovy": "groovy",
    "racket": "racket",
    "ocaml": "ocaml",
    "vbscript": "cscript //Nologo",
    "csharp": "scriptcs",
    "autoit": "autoit3",
    "autohotkey": "autohotkey",
    "ahk": "autohotkey",
    "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    "kit": "kitc --run",
    "v": "v run",
    "spwn": "spwn build",
    "pkl": "cd $dir && pkl eval -f yaml $fileName -o $fileNameWithoutExt.yaml"
  },

  // ┌─────────────────────────────────────────────────────────────────┐
  // │                 🎯 GIT & VERSION CONTROL MASTERY                │
  // │              Because commit history should tell a story         │
  // └─────────────────────────────────────────────────────────────────┘

  // INFO: 🐙 Git Core Settings - Version control done right!
  "git.path": "/usr/bin/git",
  "git.autofetch": true,
  "git.enableCommitSigning": true,
  "git.confirmSync": false,
  "git.confirmForcePush": false,
  "git.decorations.enabled": true,
  "git.inputValidation": true,
  "git.inputValidationLength": 50,
  "git.mergeEditor": true,
  "git.useEditorAsCommitInput": true,
  "git.showInlineOpenFileAction": true,
  "git.detectSubmodules": true,
  "git.openRepositoryInParentFolders": "never",

  // INFO: 📍 Git Locations & Repositories - Know where your code lives
  "git.defaultCloneDirectory": "~/Projects",
  "git.scanRepositories": [],
  "git.ignoredRepositories": ["~/Projects/legacy-repo"],

  // INFO: 💪 GitLens - Git superpowers activated!
  "gitlens.currentLine.enabled": true,
  "gitlens.codeLens.enabled": true,
  "gitlens.hovers.enabled": true,
  "gitlens.statusBar.enabled": true,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": true
  },

  // INFO: 🎨 SCM Visual Settings - Clean and minimal
  "scm.diffDecorations": "none",

